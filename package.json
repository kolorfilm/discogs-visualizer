{
  "name": "discogs-visualizer",
  "version": "2.0.0",
  "description": "An app to visualize different data from discogs.com.",
  "author": "Michael Päßler",
  "scripts": {
    "dev": "ts-node server/index.ts",
    "build": "next build",
    "start": "NODE_ENV=production ts-node server/index.ts",
    "lint": "next lint",
    "lintAll": "prettier --check . && next lint . --ext .js,.jsx,.ts,.tsx && stylelint **/*.scss && tsc --noEmit",
    "testAll": "jest --all --coverage --verbose=true",
    "prettier": "prettier --check .",
    "stylelint": "stylelint **/*.scss",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "disconnect": "^1.2.2",
    "dotenv-webpack": "^8.1.0",
    "express": "^4.19.2",
    "highcharts": "^11.4.6",
    "identity-obj-proxy": "^3.0.0",
    "next": "^14.2.5",
    "nookies": "^2.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "body-parser": "^1.20.2",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
